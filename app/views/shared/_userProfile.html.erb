<%= form_with model: current_user, url: user_registration_path, local: true do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :profile_photo %>
  <%= f.text_field :profile_pic %>

  <%= f.submit %>
<% end %>

